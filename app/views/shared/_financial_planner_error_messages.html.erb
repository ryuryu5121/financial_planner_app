<% if @financial_planner.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      <%= @financial_planner.errors.count %> 件のエラーが発生しました
    </div>
    <ul>
      <% @financial_planner.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>
